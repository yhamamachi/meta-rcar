From aa3d67e310fe5d24b8f2113b459fd2998971ac8f Mon Sep 17 00:00:00 2001
From: Valentine Barshak <valentine.barshak@cogentembedded.com>
Date: Wed, 5 May 2021 04:09:30 +0300
Subject: [PATCH 55/70] arm64: dts: renesas: ulcb-kf: Switch to HCI LL driver

Signed-off-by: Valentine Barshak <valentine.barshak@cogentembedded.com>
---
 arch/arm64/boot/dts/renesas/ulcb-kf.dtsi | 9 +++++----
 1 file changed, 5 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/renesas/ulcb-kf.dtsi b/arch/arm64/boot/dts/renesas/ulcb-kf.dtsi
index 18867a4..5ec0447 100644
--- a/arch/arm64/boot/dts/renesas/ulcb-kf.dtsi
+++ b/arch/arm64/boot/dts/renesas/ulcb-kf.dtsi
@@ -140,10 +140,6 @@
 		baud_rate = <3333332>;
 	};
 
-	btwilink {
-		compatible = "btwilink";
-	};
-
 	/delete-node/sound;
 
 	sound_card: sound@0 {
@@ -331,6 +327,11 @@
 	uart-has-rtscts;
 
 	status = "okay";
+
+	bluetooth {
+		compatible = "ti,wl1837-st";
+		enable-gpios = <&gpio_exp_74 3 GPIO_ACTIVE_HIGH>;
+	};
 };
 
 &hscif1 {
